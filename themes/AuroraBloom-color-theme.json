{
	"name": "AuroraBloom",
"colors": {
  "editor.background": "#081b25",
  "editor.foreground": "#eeffff",

  "sideBar.background": "#081b25",
  "sideBar.foreground": "#eeffff",
  "sideBarTitle.foreground": "#eeffff",
  "sideBarSectionHeader.background": "#081b25",
  
  "activityBar.background": "#081b25",
  "activityBar.foreground": "#eeffff",
  "activityBarBadge.background": "#007acc",
  "activityBarBadge.foreground": "#eeffff",

  "statusBar.background": "#081b25",
  "statusBar.foreground": "#eeffff",
  "statusBar.debuggingBackground": "#1e272c",

  "panel.background": "#081b25",
  "panel.border": "#081b25",
  "panelTitle.activeBorder": "#007acc",
  "panelTitle.activeForeground": "#eeffff",

  "terminal.background": "#081b25",
  "terminal.foreground": "#eeffff",
  "terminal.ansiBlack": "#081b25",
  "terminal.ansiBlue": "#007acc",
  "terminal.ansiBrightBlack": "#4b5c67",
  "terminal.ansiBrightBlue": "#82AAFF",
  "terminal.ansiBrightCyan": "#89DDFF",
  "terminal.ansiBrightGreen": "#C3E88D",
  "terminal.ansiBrightMagenta": "#C792EA",
  "terminal.ansiBrightRed": "#FF5370",
  "terminal.ansiBrightWhite": "#eeffff",
  "terminal.ansiBrightYellow": "#FFCB6B",
  "terminal.ansiCyan": "#89DDFF",
  "terminal.ansiGreen": "#C3E88D",
  "terminal.ansiMagenta": "#C792EA",
  "terminal.ansiRed": "#FF5370",
  "terminal.ansiWhite": "#eeffff",
  "terminal.ansiYellow": "#FFCB6B"
}
,

"tokenColors": [
  {
    "name": "Comment",
    "scope": [
      "comment",
      "punctuation.definition.comment"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#607D8B"  // slightly brighter, more blue-gray
    }
  },
  {
    "name": "Variables",
    "scope": [
      "variable",
      "string constant.other.placeholder"
    ],
    "settings": {
      "foreground": "#FFFFFF"  // pure white for better contrast
    }
  },
  {
    "name": "Colors",
    "scope": [
      "constant.other.color"
    ],
    "settings": {
      "foreground": "#FFFFFF"
    }
  },
  {
    "name": "Invalid",
    "scope": [
      "invalid",
      "invalid.illegal"
    ],
    "settings": {
      "foreground": "#FF6E6E"  // brighter red
    }
  },
  {
    "name": "Keyword, Storage",
    "scope": [
      "keyword",
      "storage.type",
      "storage.modifier"
    ],
    "settings": {
      "foreground": "#D18FFF"  // brighter, neon purple
    }
  },
  {
    "name": "Operator, Misc",
    "scope": [
      "keyword.control",
      "constant.other.color",
      "punctuation",
      "meta.tag",
      "punctuation.definition.tag",
      "punctuation.separator.inheritance.php",
      "punctuation.definition.tag.html",
      "punctuation.definition.tag.begin.html",
      "punctuation.definition.tag.end.html",
      "punctuation.section.embedded",
      "keyword.other.template",
      "keyword.other.substitution"
    ],
    "settings": {
      "foreground": "#6FDFFF"  // bright cyan/sky blue
    }
  },
  {
    "name": "Tag",
    "scope": [
      "entity.name.tag",
      "meta.tag.sgml",
      "markup.deleted.git_gutter"
    ],
    "settings": {
      "foreground": "#FF8C82"  // bright coral/red
    }
  },
  {
    "name": "Function, Special Method",
    "scope": [
      "entity.name.function",
      "meta.function-call",
      "variable.function",
      "support.function",
      "keyword.other.special-method"
    ],
    "settings": {
      "foreground": "#99CFFF"  // bright pastel blue
    }
  },
  {
    "name": "Block Level Variables",
    "scope": [
      "meta.block variable.other"
    ],
    "settings": {
      "foreground": "#FF8C82"
    }
  },
  {
    "name": "Other Variable, String Link",
    "scope": [
      "support.other.variable",
      "string.other.link"
    ],
    "settings": {
      "foreground": "#FF8C82"
    }
  },
  {
    "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
    "scope": [
      "constant.numeric",
      "constant.language",
      "support.constant",
      "constant.character",
      "constant.escape",
      "variable.parameter",
      "keyword.other.unit",
      "keyword.other"
    ],
    "settings": {
      "foreground": "#FFB678"  // bright orange
    }
  },
  {
    "name": "String, Symbols, Inherited Class, Markup Heading",
    "scope": [
      "string",
      "constant.other.symbol",
      "constant.other.key",
      "entity.other.inherited-class",
      "markup.heading",
      "markup.inserted.git_gutter",
      "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
    ],
    "settings": {
      "foreground": "#C9F29C"  // bright lime green
    }
  },
  {
  "name": "Class, Support",
  "scope": [
    "entity.name",
    "support.type",
    "support.class",
    "support.other.namespace.use.php",
    "meta.use.php",
    "support.other.namespace.php",
    "markup.changed.git_gutter",
    "support.type.sys-types"
  ],
  "settings": {
    "foreground": "#5b78ea",
    "fontStyle": "italic"
  }
}
,
  {
    "name": "Entity Types",
    "scope": [
      "support.type"
    ],
    "settings": {
      "foreground": "#B2CCD6"
    }
  },
  {
    "name": "CSS Class and Support",
    "scope": [
      "source.css support.type.property-name",
      "source.sass support.type.property-name",
      "source.scss support.type.property-name",
      "source.less support.type.property-name",
      "source.stylus support.type.property-name",
      "source.postcss support.type.property-name"
    ],
    "settings": {
      "foreground": "#B2CCD6"
    }
  },
  {
    "name": "Sub-methods",
    "scope": [
      "entity.name.module.js",
      "variable.import.parameter.js",
      "variable.other.class.js"
    ],
    "settings": {
      "foreground": "#FF6E6E"
    }
  },
  {
    "name": "Language methods",
    "scope": [
      "variable.language"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#FF6E6E"
    }
  },
  {
    "name": "entity.name.method.js",
    "scope": [
      "entity.name.method.js"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#99CFFF"
    }
  },
  {
    "name": "meta.method.js",
    "scope": [
      "meta.class-method.js entity.name.function.js",
      "variable.function.constructor"
    ],
    "settings": {
      "foreground": "#99CFFF"
    }
  },
  {
    "name": "Attributes",
    "scope": [
      "entity.other.attribute-name"
    ],
    "settings": {
      "foreground": "#D18FFF"
    }
  },
  {
    "name": "HTML Attributes",
    "scope": [
      "text.html.basic entity.other.attribute-name.html",
      "text.html.basic entity.other.attribute-name"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#FFD86B"
    }
  },
  {
    "name": "CSS Classes",
    "scope": [
      "entity.other.attribute-name.class"
    ],
    "settings": {
      "foreground": "#FFD86B"
    }
  },
  {
    "name": "CSS ID's",
    "scope": [
      "source.sass keyword.control"
    ],
    "settings": {
      "foreground": "#99CFFF"
    }
  },
  {
    "name": "Inserted",
    "scope": [
      "markup.inserted"
    ],
    "settings": {
      "foreground": "#C9F29C"
    }
  },
  {
    "name": "Deleted",
    "scope": [
      "markup.deleted"
    ],
    "settings": {
      "foreground": "#FF6E6E"
    }
  },
  {
    "name": "Changed",
    "scope": [
      "markup.changed"
    ],
    "settings": {
      "foreground": "#D18FFF"
    }
  },
  {
    "name": "Regular Expressions",
    "scope": [
      "string.regexp"
    ],
    "settings": {
      "foreground": "#6FDFFF"
    }
  },
  {
    "name": "Escape Characters",
    "scope": [
      "constant.character.escape"
    ],
    "settings": {
      "foreground": "#6FDFFF"
    }
  },
  {
    "name": "URL",
    "scope": [
      "*url*",
      "*link*",
      "*uri*"
    ],
    "settings": {
      "fontStyle": "underline"
    }
  },
  {
    "name": "Decorators",
    "scope": [
      "tag.decorator.js entity.name.tag.js",
      "tag.decorator.js punctuation.definition.tag.js"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#99CFFF"
    }
  },
  {
    "name": "ES7 Bind Operator",
    "scope": [
      "source.js constant.other.object.key.js string.unquoted.label.js"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#FF6E6E"
    }
  },
